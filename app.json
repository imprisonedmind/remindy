{
  "expo": {
    "name": "nativewind-expo-router",
    "slug": "nativewind-expo-router",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "nativewind-expo-router",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "requireFullScreen": true
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "remindy.com"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification_icon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/sounds/notification_sound.wav"
          ],
          "mode": "production",
          "android": {
            "importance": "max",
            "priority": "max",
            "useNextForegroundAction": true
          },
          "ios": {
            "critical": true,
            "timeSensitive": true,
            "infoPlist": {
              "UIBackgroundModes": ["fetch", "remote-notification"],
              "NSUserTrackingUsageDescription": "Your custom message here",
              "UIUserNotificationSettings": {
                "alert": true,
                "badge": true,
                "sound": true,
                "criticalAlert": true
              }
            }
          }
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "31374dfa-ee6d-4865-adb6-300af5056f71"
      }
    }
  }
}
